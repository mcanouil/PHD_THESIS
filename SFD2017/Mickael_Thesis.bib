	file = {Full Text PDF:D\:\\Profils\\mcanouil\\web\\firefox\\Data\\profile\\zotero\\storage\\Z25KIT8W\\Furlotte et al. - 2012 - Genome-Wide Association Mapping With Longitudinal .pdf:application/pdf}
}

@article{tu_power_2004,
