	note = {WOS:000290470200004},
	keywords = {binary data, definition, Dirichlet process prior, dropout, inference, priors, shared parameter model, shared parameter models, splines, Survival Analysis, survival-data, Time-dependent covariates},
	pages = {1366--1380},
