@article{diggle_joint_2008,
	title = {Joint modelling of repeated measurements and time-to-event outcomes: {The} fourth {Armitage} lecture},
	volume = {27},
	copyright = {Copyright © 2007 John Wiley \& Sons, Ltd.},
	issn = {1097-0258},
	shorttitle = {Joint modelling of repeated measurements and time-to-event outcomes},
	url = {http://onlinelibrary.wiley.com.doc-distant.univ-lille2.fr/doi/10.1002/sim.3131/abstract},
	doi = {10.1002/sim.3131},
	abstract = {In many longitudinal studies, the outcomes recorded on each subject include both a sequence of repeated measurements at pre-specified times and the time at which an event of particular interest occurs: for example, death, recurrence of symptoms or drop out from the study. The event time for each subject may be recorded exactly, interval censored or right censored. The term joint modelling refers to the statistical analysis of the resulting data while taking account of any association between the repeated measurement and time-to-event outcomes. In this paper, we first discuss different approaches to joint modelling and argue that the analysis strategy should depend on the scientific focus of the study. We then describe in detail a particularly simple, fully parametric approach. Finally, we use this approach to re-analyse data from a clinical trial of drug therapies for schizophrenic patients, in which the event time is an interval-censored or right-censored time to withdrawal from the study due to adverse side effects. Copyright © 2007 John Wiley \& Sons, Ltd.},
	language = {en},
	number = {16},
	urldate = {2015-01-26},
	journal = {Statistics in Medicine},
	author = {Diggle, Peter J. and Sousa, Inês and Chetwynd, Amanda G.},
	year = {2008},
	keywords = {joint modelling, longitudinal analysis, time to event},
	pages = {2981--2998}
}

