	file = {PLoS Full Text PDF:D\:\\Profils\\mcanouil\\web\\firefox\\Data\\profile\\zotero\\storage\\FBN52IUK\\Smith et al. - 2010 - Longitudinal Genome-Wide Association of Cardiovasc.pdf:application/pdf}
