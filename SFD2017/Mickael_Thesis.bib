@article{verbeke_effect_2007,
	title = {The effect of miss-specified baseline characteristics on inference for longitudinal trends in linear mixed models},
	volume = {8},
	issn = {1465-4644, 1468-4357},
	url = {http://biostatistics.oxfordjournals.org/content/8/4/772},
	doi = {10.1093/biostatistics/kxm004},
	abstract = {The main advantage of longitudinal studies is that they can distinguish changes over time within individuals (longitudinal effects) from differences among subjects at the start of the study (baseline characteristics, cross-sectional effects). Often, especially in observational studies, longitudinal trends are studied after correction for many potentially important baseline differences between subjects. We show that, in the context of linear mixed models, inference for longitudinal trends is in general biased if a wrong model for the baseline characteristics is used. However, we will argue that this bias is small in most practical situations and completely vanishes in the special case of a growth curve model for complete balanced data. In the latter case, inference for longitudinal trends is completely independent of additional baseline covariates that might have been omitted from the model.},
	language = {en},
	number = {4},
	urldate = {2015-10-25},
	journal = {Biostatistics},
	author = {Verbeke, Geert and Fieuws, Steffen},
	month = oct,
